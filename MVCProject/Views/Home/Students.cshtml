@model List<MVCProject.Models.StudentM>

@{
    ViewData["Title"] = "Students";
}

<h1>Students</h1>

<p>
    <a asp-action="AllStudents" class="btn btn-primary">Display All Students</a>
</p>

<form asp-action="DisplayStudent" method="get" class="form-inline">
    <div class="form-group">
        <label for="studentId">Student ID:</label>
        <input type="number" class="form-control" id="studentId" name="id" required />
    </div>
    <button type="submit" class="btn btn-primary">Display Specific Student</button>
</form>


